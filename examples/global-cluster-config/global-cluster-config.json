{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "This template creates a GlobalClusterConfig resource  on the MongoDB Atlas API, this will be billed to your Atlas account.",
  "Parameters": {
    "ProjectId": {
      "Type": "String",
      "Description": "",
      "Default": "633bf97a5684466a4793c55c",
      "ConstraintDescription": ""
    },
    "ClusterName": {
      "Type": "String",
      "Description": "",
      "Default": "cfntest1",
      "ConstraintDescription": ""
    },
    "Db": {
      "Type": "String",
      "Description": "",
      "Default": "cfntest1",
      "ConstraintDescription": ""
    },
    "Collection": {
      "Type": "String",
      "Description": "",
      "Default": "cfntest1",
      "ConstraintDescription": ""
    },
    "Profile": {
      "Type": "String",
      "Description": "Atlas Profile name",
      "Default": "default"
    }
  },
  "Mappings": {},
  "Resources": {
    "GlobalClusterConfig": {
      "Type": "MongoDB::Atlas::GlobalClusterConfig",
      "Properties": {
        "Profile": {
          "Ref": "Profile"
        },
        "ProjectId": {
          "Ref": "ProjectId"
        },
        "ClusterName": {
          "Ref": "ClusterName"
        },
        "ManagedNamespaces": [
          {
            "Collection": {
              "Ref": "Collection"
            },
            "CustomShardKey": "shardkey1",
            "Db": {
              "Ref": "Db"
            },
            "IsCustomShardKeyHashed": "false",
            "IsShardKeyUnique": "false"
          }
        ],
        "CustomZoneMappings": [
          {
            "Location": "US",
            "Zone": "Zone 1"
          }
        ],
        "RemoveAllZoneMapping": false
      }
    }
  },
  "Outputs": {
    "GlobalClusterConfig": {
      "Description": "Id of the GlobalClusterConfig resource",
      "Value": {
        "Ref": "GlobalClusterConfig"
      }
    }
  }
}
