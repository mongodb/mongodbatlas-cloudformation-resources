{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "This template creates a PrivateEndpoint on the MongoDB Atlas API",
  "Parameters": {
    "PublicKey": {
      "Type": "String"
    },
    "Privatekey": {
      "Type": "String"
    },
    "GroupId": {
      "Type": "String"
    },
    "Region": {
      "Type": "String"
    }
  },
  "Resources": {
    "PrivateEndpointService": {
      "Type": "MongoDB::Atlas::PrivateEndpoint",
      "Properties": {
        "GroupId": { "Ref" : "GroupId" },
        "Region": { "Ref" : "Region" },
        "ApiKeys": {
          "PublicKey": { "Ref" : "PublicKey" },
          "PrivateKey": { "Ref" : "Privatekey" }
        }
      }
    }
  },
  "Outputs": {}
}