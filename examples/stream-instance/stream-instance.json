{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Returns, adds, edits, or removes a stream instance, this will be billed to your Atlas account.",
  "Mappings": {},
  "Parameters": {
    "Profile": {
      "Type": "String",
      "Description": "MongoDB Atlas Profile name"
    },
    "GroupId": {
      "Type": "String"
    },
    "Name": {
      "Type": "String"
    },
    "Tier": {
      "Type": "String"
    },
    "Id": {
      "Type": "String"
    }
  },
  "Resources": {
    "StreamInstance": {
      "Type": "MongoDB::Atlas::StreamInstance",
      "Properties": {
        "Profile": {
          "Ref": "Profile"
        },
        "GroupId": {
          "Ref": "GroupId"
        },
        "Name": {
          "Ref": "Name"
        },
        "DataProcessRegion": {
          "CloudProvider": "AWS",
          "Region": "VIRGINIA_USA"
        },
        "StreamConfig": {
          "Tier": {
            "Ref": "Tier"
          }
        },
        "Id": {
          "Ref": "Id"
        }
      }
    }
  },
  "Outputs": {
    "Id": {
      "Value": {
        "Ref": "Id"
      }
    }
  }
}
