{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "MongoDB Atlas AWS CloudFormation Example for MongoDB::Atlas::ThirdPartyIntegration",
  "Parameters": {
    "Profile": {
      "Type": "String",
      "Default": "default"
    },
    "ProjectId": {
      "Description": "Your MongoDB Project Id",
      "Type": "String",
      "Default": ""
    }
  },
  "Resources": {
    "DatadogIntegration": {
      "Type": "MongoDB::Atlas::ThirdPartyIntegration",
      "Properties": {
        "Profile": {
          "Ref": "Profile"
        },
        "ProjectId": {
          "Ref": "ProjectId"
        },
        "Type": "DATADOG",
        "ApiKey": "********************************",
        "Region": "US",
        "SendUserProvidedResourceTags": true,
        "SendCollectionLatencyMetrics": false,
        "SendDatabaseMetrics": false
      }
    }
  }
}
