{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "MongoDB Atlas AWS CloudFormation Example for MongoDB::Atlas::ThirdPartyIntegration",
  "Parameters": {
    "PublicKey": {
      "Description": "Your MongoDB Cloud Public API Key",
      "Type": "String",
      "Default": ""
    },
    "PrivateKey": {
      "Description": "Your MongoDB Cloud Private API Key",
      "Type": "String",
      "Default": ""
    },
    "ProjectId": {
      "Description": "Your MongoDB Project Id",
      "Type": "String",
      "Default": ""
    }
  },
  "Resources": {
    "PrometheusIntegration": {
      "Type": "MongoDB::Atlas::ThirdPartyIntegration",
      "Properties": {
        "ApiKeys": {
          "PublicKey": {
            "Ref": "PublicKey"
          },
          "PrivateKey": {
            "Ref": "PrivateKey"
          }
        },
        "ProjectId": {
          "Ref": "ProjectID"
        },
        "Type": "PROMETHEUS",
        "Enabled": true,
        "Scheme": "http",
        "ServiceDiscovery": "http",
        "UserName": "647845",
        "Password": "eyJrIjoiYWZiNzU0YjkyOTI4YjM1ZTQzYWEyMzhkYjk3OTlmYmUxYzU2MzAzNiIsIm4iOiJ0ZXN0IiwiaWQiOjc0MjU3N30="
      }
    }
  }
}